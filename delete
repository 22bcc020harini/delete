  docker network inspect bridge
  docker network create --driver bridge kcw
  docker network ls
  docker pull centos
  docker run -it --name first_centos centos
  docker network connect kcw first_centos
  docker network container ls -a
  docker network inspect kcw
  docker network disconnect kcw first_centos
  docker network inpect kcw
  docker network rm kcw
  docker network ls
  
